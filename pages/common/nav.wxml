<template name="nav">
  <view class="weui-flex topbar">
    <!-- 排列商品分类title S -->
    <block wx:for="{{ navList }}" wx:key="">
      <view data-type="{{ item.id }}" data-index="{{ index }}" class="weui-flex__item" catchtap="onTapTag">
        <view class="topbar-item {{ index == activeIndex ? 'active' : '' }}">
          {{ item.name }}</view>
      </view>
    </block>
    <!-- 排列商品分类title E -->

  </view>
</template>